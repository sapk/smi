{
  "name": "simple-monitor-interface",
  "description": "A simple interface to monitor your personal cluster.",
  "author": "Antoine GIRARD <sapk@sapk.fr>",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --host=localhost --port=8081",
    "build": "rm -Rf dist && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "filesize": "^3.3.0",
    "firebase": "^3.6.4",
    "moment": "^2.17.1",
    "vue": "^2.1.8"
  },
  "devDependencies": {
    "buble": "^0.15.1",
    "buble-loader": "^0.4.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "firebase-tools": "^3.2.1",
    "pug": "^2.0.0-beta6",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "vue-loader": "^10.0.2",
    "vue-router": "^2.1.1",
    "vue-template-compiler": "^2.1.8",
    "vuetify": "^0.8.*",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.1.0",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
